<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:id="@+id/NRFLinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical">
	<include android:layout_width="wrap_content"
		android:layout_height="wrap_content" layout="@layout/title_comp"
		android:id="@+id/TitleComp"></include>
	<LinearLayout android:id="@+id/NRFLinearLayout04"
		android:orientation="vertical" android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<LinearLayout android:id="@+id/NRFLinearLayout02"
			android:layout_height="wrap_content" android:layout_width="fill_parent"
			android:gravity="center_horizontal" android:orientation="horizontal"
			android:layout_weight="0">
			<ImageView android:id="@+id/NRFImageView01"
				android:layout_height="75dp" android:background="@drawable/button_border"
				android:paddingBottom="2dp" android:paddingTop="2dp"
				android:layout_width="115dp" android:layout_marginRight="5dp"></ImageView>
			<ImageView android:id="@+id/NRFImageView02"
				android:layout_height="75dp" android:background="@drawable/button_border"
				android:paddingBottom="4dp" android:paddingTop="4dp"
				android:layout_width="115dp" android:layout_marginLeft="5dp"></ImageView>
		</LinearLayout>

		<ScrollView android:id="@+id/NRFScrollView01"
			android:layout_height="wrap_content" android:scrollbars="none"
			android:layout_width="fill_parent" android:layout_weight="0.3">
			<LinearLayout android:id="@+id/NRFLinearLayout03"
				android:layout_height="wrap_content" android:orientation="vertical"
				android:layout_width="fill_parent" android:paddingLeft="30dp"
				android:paddingRight="30dp">

				<TextView android:id="@+id/NRFTextView01"
					android:layout_height="wrap_content" android:layout_width="fill_parent"
					android:textColor="@color/descN" android:text="@string/NewRateFormTV1"
					android:layout_marginTop="9dp"></TextView>
				<com.riktamtech.android.ratethisstc.ui.components.CustDropDown
					android:layout_height="wrap_content" android:id="@+id/NRFTextView101"
					android:layout_width="fill_parent"></com.riktamtech.android.ratethisstc.ui.components.CustDropDown>
				<TableLayout android:id="@+id/tableLayout1"
					android:layout_width="match_parent" android:layout_height="wrap_content">
					<TableRow android:id="@+id/tableRow1"
						android:layout_width="wrap_content" android:layout_height="wrap_content">
						<TextView android:layout_width="fill_parent"
							android:layout_height="wrap_content" android:id="@+id/NRFTextView02"
							android:textColor="@color/descN" android:layout_marginTop="4dp"
							android:text="@string/NewRateFormLabelPictureA"></TextView>
						<TextView android:layout_height="wrap_content"
							android:layout_width="wrap_content" android:id="@+id/textView1"
							android:textColor="@color/descN" android:layout_marginTop="4dp"
							android:layout_marginLeft="5dp" android:text="@string/NewRateFormLabelPictureB"></TextView>
					</TableRow>
					<TableRow android:id="@+id/tableRow2"
						android:layout_width="wrap_content" android:layout_height="wrap_content">
						<EditText android:paddingLeft="2dp" android:textColor="#ffffff"
							android:imeOptions="actionDone" android:layout_height="wrap_content"
							android:background="@color/tvBg" android:id="@+id/NRFEditText02"
							android:textSize="15sp" android:layout_width="130dp" android:ellipsize="none"></EditText>
						<EditText android:id="@+id/editText1"
							android:layout_height="wrap_content" android:background="@color/tvBg"
							android:textSize="15sp" android:textColor="#ffffff"
							android:layout_marginLeft="5dp" android:layout_width="130dp" android:ellipsize="none"></EditText>
					</TableRow>
				</TableLayout>
				<TextView android:id="@+id/NRFTextView03"
					android:layout_height="wrap_content" android:layout_width="fill_parent"
					android:textColor="@color/descN" android:text="@string/NewRateFormTV3"
					android:layout_marginTop="4dp"></TextView>
				<com.riktamtech.android.ratethisstc.ui.components.CustDropDown
					android:id="@+id/NRFTextView102" android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:background="@drawable/combo"></com.riktamtech.android.ratethisstc.ui.components.CustDropDown>
				<TextView android:id="@+id/NRFTextView04"
					android:layout_height="wrap_content" android:layout_width="fill_parent"
					android:textColor="@color/descN" android:text="@string/NewRateFormTV4"
					android:layout_marginTop="4dp"></TextView>
				<LinearLayout android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/NRFLinearLayout001"
					android:gravity="left">
					<com.riktamtech.android.ratethisstc.ui.components.CustDropDown
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_weight="0.2" android:id="@+id/NRFTextView103"></com.riktamtech.android.ratethisstc.ui.components.CustDropDown>
					<com.riktamtech.android.ratethisstc.ui.components.CustDropDown
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_marginLeft="10dp" android:id="@+id/NRFTextView104"
						android:layout_weight="0.1" android:visibility="gone"></com.riktamtech.android.ratethisstc.ui.components.CustDropDown>
					<com.riktamtech.android.ratethisstc.ui.components.CustDropDown
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_marginLeft="10dp" android:id="@+id/NRFTextView1041"
						android:layout_weight="0.1" android:visibility="gone"></com.riktamtech.android.ratethisstc.ui.components.CustDropDown>
				</LinearLayout>
				<TextView android:id="@+id/NRFTextView05"
					android:layout_height="wrap_content" android:layout_width="fill_parent"
					android:textColor="@color/descN" android:text="@string/NewRateFormTV5"
					android:layout_marginTop="4dp"></TextView>
				<com.riktamtech.android.ratethisstc.ui.components.CustDropDown
					android:id="@+id/NRFTextView105" android:layout_height="wrap_content"
					android:layout_width="fill_parent"></com.riktamtech.android.ratethisstc.ui.components.CustDropDown>
				<ImageButton android:id="@+id/NRFImageButton01"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/post" android:background="#00000000"
					android:layout_gravity="center_horizontal" android:layout_weight="0"
					android:layout_marginBottom="3dp" android:layout_marginTop="20dp"></ImageButton>


			</LinearLayout>
		</ScrollView>
	</LinearLayout>
</LinearLayout>
