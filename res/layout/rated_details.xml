<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:id="@+id/FrameLayout01"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent">
	<LinearLayout android:id="@+id/RatedDetailsLinearLayout01"
		android:layout_width="fill_parent" android:layout_height="fill_parent"

		android:orientation="vertical">
		<include android:layout_height="wrap_content" android:id="@+id/TitleComp"
			layout="@layout/title_comp" android:layout_width="fill_parent"></include>
		<com.riktamtech.android.ratethisstc.ui.components.RateitGallery android:id="@+id/Gallery01" android:layout_width="fill_parent"
			android:animationDuration="10" android:layout_height="fill_parent"></com.riktamtech.android.ratethisstc.ui.components.RateitGallery>
	</LinearLayout>


	<include layout="@layout/share_op" android:id="@+id/ShareOp"
		android:layout_gravity="bottom" android:layout_width="fill_parent"
		android:layout_height="wrap_content"></include>

</FrameLayout>